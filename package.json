    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "axios-cookiejar-support": "^4.0.3",
    "tough-cookie": "^4.1.2"
