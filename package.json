{
  "name": "librus.ts",
  "version": "1.0.0",
  "description": "Library for interacting with the Librus API",
  "main": "index.js",
  "types": "index.d.ts",
  "readme": "README.md",
  "scripts": {
    "format": "prettier --config .prettierrc ./src/**/*.ts --write",
    "lint": "eslint --color ./src/**/*.ts",
    "prebuild": "npm run format && npm run lint",
    "build": "npm run prebuild && tsc",
    "publish": "rm -rf dist && npm run build && cp package.json dist && npm publish ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakhildev/librus.ts.git"
  },
  "keywords": [
    "Librus",
    "API"
  ],
  "author": "Zakhil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakhildev/librus.ts/issues"
  },
  "devDependencies": {
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "axios-cookiejar-support": "^4.0.3",
    "tough-cookie": "^4.1.2"
  }
}
